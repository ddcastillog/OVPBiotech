    width: 500px;
    height: 90px;
    -unity-font-style: normal;
    font-size: 39px;
    color: rgb(255, 255, 255);
    -unity-text-align: middle-center;
    background-color: rgb(0, 0, 0);
    background-image: url('project://database/Assets/Art%20Gameplay/2x/musicOn.png?fileID=21300000&guid=19998deea48ab3e4c9259a29407173ad&type=3#musicOn');
